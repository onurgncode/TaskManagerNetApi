@{
    Layout = "_Layout";
    @model List<Gorev>;

}
<table class="table">
    <thead>
        <tr>

            <th scope="col">#</th>
            <th scope="col">Başlık</th>
            <th scope="col">Kaç Günlük</th>
            <th scope="col">Oluşturulma Tarihi</th>
            <th scope="col">Yapılacak</th>
        </tr>
    </thead>
    <tbody>
        
            @foreach (var gorev in Model)
            {
            <tr>
                <th scope="row">@gorev.id</th>
                <td>@gorev.baslik</td>
                <td>@gorev.tarih</td>
                <td>@gorev.birthday.ToString("dd.MM.yyyy")</td>
                <td>@gorev.yazigirin</td>
            </tr>
            }
           
        
        
    </tbody>
</table>